<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SerialController">
   <top>
      <HBox alignment="CENTER" prefHeight="50.0" BorderPane.alignment="CENTER">
         <children>
            <CheckBox fx:id="checkBoxOn" mnemonicParsing="false" prefHeight="50.0" prefWidth="250.0" selected="true" text="On">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" top="5.0" />
               </HBox.margin></CheckBox>
            <ChoiceBox fx:id="choiceBoxPort" onAction="#choiceBoxPortAction" prefHeight="50.0" prefWidth="500.0">
               <HBox.margin>
                  <Insets bottom="5.0" right="10.0" top="5.0" />
               </HBox.margin>
            </ChoiceBox>
            <Button fx:id="buttonClear" mnemonicParsing="false" onAction="#buttonClearAction" prefHeight="50.0" prefWidth="250.0" text="Clear">
               <HBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="buttonBack" mnemonicParsing="false" onAction="#buttonBackAction" prefHeight="50.0" prefWidth="250.0" text="Back">
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
   </top>
   <center>
      <TextArea fx:id="textAreaSerial" editable="false" prefHeight="400.0" prefWidth="500.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
