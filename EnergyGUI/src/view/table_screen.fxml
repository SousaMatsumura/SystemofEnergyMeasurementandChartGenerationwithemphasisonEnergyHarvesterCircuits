<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TableController">
   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox prefHeight="80.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="200.0" prefWidth="1000.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="40.0" prefWidth="1000.0" text="Date Begin" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                        <TextField fx:id="textFieldDateBegin" minWidth="80.0" prefHeight="30.0" prefWidth="1000.0" promptText="yyyy-MM-dd" text="any" />
                        <Label alignment="CENTER" prefHeight="40.0" prefWidth="1000.0" text="Date End" />
                        <TextField fx:id="textFieldDateEnd" minWidth="80.0" prefHeight="30.0" prefWidth="1000.0" promptText="yyyy-MM-dd" text="any" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="40.0" prefWidth="1000.0" text="Time Begin" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                        <TextField fx:id="textFieldTimeBegin" prefHeight="30.0" prefWidth="1000.0" promptText="hh:mm:ss.SSSS" text="any" />
                        <Label alignment="CENTER" prefHeight="40.0" prefWidth="1000.0" text="Time End" />
                        <TextField fx:id="textFieldTimeEnd" minWidth="80.0" prefHeight="30.0" prefWidth="1000.0" promptText="hh:mm:ss.SSSS" text="any" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="80.0" prefWidth="900.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="80.0" prefWidth="250.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                        <CheckBox fx:id="checkBoxVoltage" mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0" selected="true" text="Voltage" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                        <CheckBox fx:id="checkBoxCurrent" mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0" selected="true" text="Current" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                        <CheckBox fx:id="checkBoxPower" mnemonicParsing="false" prefHeight="30.0" prefWidth="1000.0" selected="true" text="Power" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="buttonBack" alignment="CENTER" mnemonicParsing="false" onAction="#buttonBackAction" prefHeight="30.0" prefWidth="500.0" text="Back" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                        <Button fx:id="buttonSearch" mnemonicParsing="false" onAction="#buttonSearchAction" prefHeight="30.0" prefWidth="500.0" text="Search" />
                        <Label prefHeight="17.0" prefWidth="15.0" text="  " />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </top>
</BorderPane>
